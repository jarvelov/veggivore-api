{
  "name": "Veggivore-API",
  "version": "1.0.0",
  "description": "Backend for Veggivore project",
  "author": "Tobias Järvelöv <tobias@jarvelov.se>",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development node dev-server.js",
    "postinstall": "npm rebuild",
    "build": "node node_modules/webpack/bin/webpack.js -p --config ./webpack.production.config.js",
    "serve": "NODE_ENV=production node node_modules/nodemon/bin/nodemon.js build/server.js",
    "prod": "npm run build && npm run serve",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js src test/unit/specs test/e2e/specs"
  },
  "dependencies": {
    "bcrypt": "^0.8.7",
    "bcrypt-as-promised": "^1.1.0",
    "client-sessions": "^0.7.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc-webpack-plugin": "^0.0.1",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.17.2",
    "mongoose": "^4.6.8",
    "mongoose-autopopulate": "^0.4.0",
    "mongoose-schema-extend": "^0.2.2",
    "node-restify-validation": "^1.0.5",
    "nodemon": "^1.11.0",
    "passport-jwt": "^2.1.0",
    "passport-local": "^1.0.0",
    "passport-restify": "^1.0.1",
    "restify": "^4.1.1",
    "restify-jwt": "^0.4.0",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "clean-webpack-plugin": "^0.1.13",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-promise": "^3.3.2",
    "eslint-plugin-standard": "^2.0.1",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-node-externals": "^1.5.4"
  }
}
